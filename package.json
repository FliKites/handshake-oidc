{
  "name": "handshake-oidc",
  "description": "OIDC provider using handshake name resolution",
  "version": "0.0.1",
  "config": {
    "deploymentName": "namernews-jx-dev-oidc",
    "namespace": "namernews-jx-dev"
  },
  "scripts": {
    "dev": "NODE_ENV=development PORT=8080 sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "dev:remote": "npm run dev:remote:shell -- --run npm run dev",
    "dev:remote:shell": "telepresence --swap-deployment $npm_package_config_deploymentName --namespace $npm_package_config_namespace --logfile /dev/null"
  },
  "dependencies": {
    "@sentry/integrations": "^6.1.0",
    "@sentry/node": "^6.1.0",
    "compression": "^1.7.4",
    "connect-redis": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.5",
    "express-session": "^1.17.1",
    "hdns": "^0.7.0",
    "helmet": "^4.4.1",
    "ioredis": "^4.22.0",
    "morgan": "^1.10.0",
    "oidc-provider": "^6.31.0",
    "polka": "next",
    "sirv": "^1.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.1",
    "@rollup/plugin-url": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/express-session": "^1.17.3",
    "@types/node": "^14.14.25",
    "@types/polka": "^0.5.2",
    "rollup": "^2.38.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "svelte": "^3.32.3",
    "svelte-check": "^1.1.34",
    "svelte-preprocess": "^4.6.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  }
}